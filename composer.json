{
    "name": "commnerd/apf",
    "description": "Antiquated PHP Framework designed to work with PHP ~5.3",
    "keywords": ["framework", "apf"],
    "type": "project",
    "require": {
        "altorouter/altorouter" : "1.1.0",
        "phpseclib/phpseclib" : "2.0.0",
        "ircmaxell/random-lib" : "1.0",
        "codeguy/upload" : "1.3.2",
        "twig/twig" : "~1.0",
        "phpoffice/phpexcel" : "1.8.1"
    },
    "autoload" : {
        "psr-4" : {
                "App\\" : "app/",
                "System\\" : "System/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"cd config && cp global.ini.example global.ini && cp database.ini.example database.ini && cp templating.ini.example templating.ini;\""
        ]
    },
    "config": {
        "preferred-install": "dev",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
